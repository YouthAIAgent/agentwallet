{
  "name": "aw-protocol-sdk",
  "version": "0.4.0",
  "description": "TypeScript SDK for AgentWallet Protocol - AI Agent Wallet Infrastructure on Solana",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": ["ai", "agent", "wallet", "solana", "sdk", "acp", "swarm", "web3", "defi"],
  "author": "AgentWallet Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/YouthAIAgent/agentwallet"
  },
  "homepage": "https://agentwallet.fun",
  "bugs": {
    "url": "https://github.com/YouthAIAgent/agentwallet/issues"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
